{
  "name": "vue3-video-player",
  "version": "1.0.0",
  "description": "A video player component based on video.js for vue3",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlisaLiCn/vue3-video-player.git"
  },
  "keywords": [
    "vue3",
    "video-player",
    "video",
    "player"
  ],
  "author": "alisalicn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlisaLiCn/vue3-video-player/issues"
  },
  "homepage": "https://github.com/AlisaLiCn/vue3-video-player#readme",
  "dependencies": {
    "video.js": "^7.17.0",
    "vue": "^3.2.27"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "typescript": "~4.5.4",
    "unplugin-auto-import": "^0.5.11",
    "vite": "^2.7.13",
    "vite-plugin-eslint": "^1.3.0",
    "vue-tsc": "^0.29.8"
  }
}
